{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "programId": {
          "type": "integer"
        },
        "programName": {
          "type": "string"
        },
        "programDescription": {
          "type": "string"
        },
        "programStatus": {
          "type": "string"
        },
        "programUsers": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "userId": {
                  "type": "string"
                },
                "userFirstName": {
                  "type": "string"
                },
                "userLastName": {
                  "type": "string"
                },
                "userMiddleName": {
                  "type": ["string", "null"]
                },
                "userPhoneNumber": {
                  "type": "string"
                },
                "userLocation": {
                  "type": ["string", "null"]
                },
                "userTimeZone": {
                  "type": ["string", "null"]
                },
                "userLinkedinUrl": {
                  "type": ["string", "null"]
                },
                "userEduUg": {
                  "type": ["string", "null"]
                },
                "userEduPg": {
                  "type": ["string", "null"]
                },
                "userComments": {
                  "type": ["string", "null"]
                },
                "userVisaStatus": {
                  "type": ["string", "null"]
                },
                "userLoginEmail": {
                  "type": "string"
                }
              },
              "required": [
                "userId",
                "userFirstName",
                "userLastName",
                "userPhoneNumber",
                "userLoginEmail"
              ]
            }
          ]
        },
        "creationTime": {
          "type": "string"
        },
        "lastModTime": {
          "type": "string"
        }
      },
      "required": [
        "programId",
        "programName",
        "programDescription",
        "programStatus",
        "programUsers",
        "creationTime",
        "lastModTime"
      ]
    }
  ]
}